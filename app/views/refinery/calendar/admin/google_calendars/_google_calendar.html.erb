<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(google_calendar) -%>">
  <span class='title'>
    <%= google_calendar.google_calendar_id %>

  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_calendar_admin_google_calendar_path(google_calendar),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.calendar_admin_google_calendar_path(google_calendar),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => google_calendar.google_calendar_id),
        :method => :delete %>
  </span>
</li>
