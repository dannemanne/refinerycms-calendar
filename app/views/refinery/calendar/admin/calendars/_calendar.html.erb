<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(calendar) -%>">
  <span class='title'>
    <%= calendar.title %>

  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_calendar_admin_calendar_path(calendar),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.calendar_admin_calendar_path(calendar),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => calendar.title),
        :method => :delete %>
  </span>
</li>
